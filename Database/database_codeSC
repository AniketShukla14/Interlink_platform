

CREATE TABLE School (
    ST_CT_SC_Code VARCHAR(25),
    Session DATE NOT NULL,
    Active ENUM('Y','N') ,
    RegdNo VARCHAR(20) NOT NULL,
    DOJ DATE NOT NULL,
    Name CHAR(20),
    DOB DATE NOT NULL,
    Gender ENUM('M','F'),
    Class1 INT(25),  
    Class2 INT(25),    
    Class3 INT(25),
    Class4 INT(25),
    Class5 INT(25),
    Class6 INT(25),
    Class7 INT(25),
    Class8 INT(25),
    Class9 INT(25),
    Class10 INT(25),
    DOL DATE NOT NULL,
    ShiftTo VARCHAR(25)
  );


